				Comments.addCommentMy(entry_id, last, 0);
			}
			setTimeout("CommentPoller.check(3272161);", 30000);
		}
	}
	CommentPoller.check(3272161);
	/* ShareBox.ad("ad_email"); */
</script>                </div>

