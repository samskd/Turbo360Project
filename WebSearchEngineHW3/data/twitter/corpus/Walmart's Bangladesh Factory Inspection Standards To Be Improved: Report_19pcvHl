				var last=document.getElementById('lastComment').innerHTML;
				Comments.addCommentMy(entry_id, last, 0);
			}
			setTimeout("CommentPoller.check(3274900);", 30000);
		}
	}
	CommentPoller.check(3274900);
	/* ShareBox.ad("ad_email"); */
</script>                </div>

