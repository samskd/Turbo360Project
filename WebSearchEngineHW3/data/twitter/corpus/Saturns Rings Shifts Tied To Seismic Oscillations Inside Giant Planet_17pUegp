				Comments.addCommentMy(entry_id, last, 0);
			}
			setTimeout("CommentPoller.check(3271835);", 30000);
		}
	}
	CommentPoller.check(3271835);
	/* ShareBox.ad("ad_email"); */
</script>                </div>

