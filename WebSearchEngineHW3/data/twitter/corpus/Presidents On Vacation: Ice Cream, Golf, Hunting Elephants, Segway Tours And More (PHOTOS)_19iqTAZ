				$( '#slideshow_json_3cc04ddcb56a46d4a8d273cc4f9fc1cb' ).data( 'slideshow', data );
				if ( --requests <= 0 ) render();
			} });
			
						
			require.config( { 
				baseUrl : 'http://s.huffpost.com/include/lib/',
				paths   : { 'slideshows' : '_slideshows/modules', 'slideshow_styles' : '_slideshows/modules' }
			} );
