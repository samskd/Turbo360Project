                    HPAds.page_spots["adtech_header"]=true; HPAds.page_spots["asset_inserts"]=true; HPAds.page_spots["sharebox_260x60"]=true; HPAds.page_spots["social_sponsor_campaigns"]=true; HPAds.page_spots["big_news_link"]=true; HPAds.page_spots["leaderboard_flex"]=true; HPAds.page_spots["leaderboard_top"]=true; HPAds.page_spots["curtain"]=true; HPAds.page_spots["slideshow_300x250_req"]=true; HPAds.page_spots["bottom_article_text"]=true; HPAds.page_spots["right_rail_flex"]=true; HPAds.page_spots["sponsorship_2"]=true; HPAds.page_spots["btf_300x100"]=true; 
                </script>		<!-- footer -->
	</div>
