            <script type="text/javascript">
                function centrechosenimage() {
                    var ph=$('#mosaictemplateimage').parent().height();
                    var pw=$('#mosaictemplateimage').parent().width();
                    var ih=$('#mosaictemplateimage').height();
                    var iw=$('#mosaictemplateimage').width();
                    if(ih>=ph) {
                        $('#mosaictemplateimage').css('margin-top',0);
                    } else {
                        var tm=(ph-ih)/2;
                        $('#mosaictemplateimage').css('margin-top',tm);
                    }
                    if(iw>=pw) {
                        $('#mosaictemplateimage').css('margin-left',0);
                    } else {
                        var lm=(pw-iw)/2;
                        $('#mosaictemplateimage').css('margin-left',lm);
                    }
                }

                $('#mosaictemplateimage').load(function () {
                    centrechosenimage();
                });

                jQuery(document).ready(function () {
                    jQuery('#mosa').lightBox();
                });
            </script>
