	<div id="badges_v2_1"  ><!-- Badge layout is: 10 --></div>

	
	<script type="text/javascript">
	
	
		var inst_def = Badges.getUniqName();

		window[inst_def.obj_name] = new Badges({
			unique_id: inst_def.unique_id,
