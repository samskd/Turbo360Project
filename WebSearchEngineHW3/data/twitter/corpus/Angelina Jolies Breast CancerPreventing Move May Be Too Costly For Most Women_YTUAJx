				jQuery(document).ready(function() {
					jQuery("#img_caption_3273594").mouseenter(function(){
					  jQuery('#caption_3273594').fadeIn(100);
					}).mouseleave(function(e){
					    if( e.relatedTarget.id == 'caption_3273594' ) return false;
						jQuery('#caption_3273594').fadeOut(100);
					});
					jQuery('#caption_3273594').mouseleave(function(e){
						if( e.relatedTarget.id != 'img_caption_3273594' )
