			<script type="text/javascript">
				jQuery(document).ready(function() {
					jQuery("#img_caption_3274377").mouseenter(function(){
					  jQuery('#caption_3274377').fadeIn(100);
					}).mouseleave(function(e){
					    if( e.relatedTarget.id == 'caption_3274377' ) return false;
						jQuery('#caption_3274377').fadeOut(100);
					});
					jQuery('#caption_3274377').mouseleave(function(e){
						if( e.relatedTarget.id != 'img_caption_3274377' )
							jQuery('#caption_3274377').fadeOut(100);
