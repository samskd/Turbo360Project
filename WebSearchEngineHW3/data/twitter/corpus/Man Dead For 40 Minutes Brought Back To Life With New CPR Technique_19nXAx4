<script>
var map_data = {"point":{"lng":145.218216,"lat":-37.98531},"center":{"lat":-37.88916100483482,"lng":144.94084849002033},"zoom":5,"city":"Dandenong","country":"AU","hover":"Dandenong, VIC, AU"};
jQuery(document).ready(function () {
  jQuery('#entry_map').html('');

  var options = {
    elt: document.getElementById("entry_map"),
