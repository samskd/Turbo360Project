        <script type="text/javascript">
            function newFilledArray(length, val) {
                var array = [];
                for (var i = 0; i < length; i++) {
                    array[i] = val;
                }
                return array;
            }

            var tearOffDetails = newFilledArray(10, { Name: "rachel henderson's", pageURL: "DrHoness" });
            $('#tear-off-template').tmpl(tearOffDetails).appendTo('#tear-off-strip-container');

