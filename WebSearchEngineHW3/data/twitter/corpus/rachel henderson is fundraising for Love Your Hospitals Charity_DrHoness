<script type="text/javascript">

    if (JustGiving.Analytics.RenderGoogleAnalyticsInDomReady) {
        jQuery(function () {
            renderAnalytics();
        });
    } else {
        renderAnalytics();
    }

    function renderAnalytics() {
        try {
            JustGiving.Analytics.push(['_setAccount', 'UA-11682530-1']);
            JustGiving.Analytics.push(['_setDomainName', 'justgiving.com']);
            JustGiving.Analytics.push(['_setCustomVar', 2, 'Region selection', 'GB-GBP', 1]);
            

            if (!JustGiving.Analytics.SuppressCookiePreferencesVariable) {
                 JustGiving.Analytics.push(['_setCustomVar', 4, 'cookie preferences', 'displayed-Tiny', 1]);
            }

            JustGiving.Analytics.TrackFacebookSourceAndMedium.Push(['fb_source'], ['fb_ref', 'fb_action_types']);

            if (JustGiving.Analytics.RegisterCustomVars) {
                JustGiving.Analytics.RegisterCustomVars();
            }

        JustGiving.Analytics.push(['_trackPageview', '/fundraisingpagenew/DrHoness?targetdevice=web']);
JustGivingAnalyticsInitialPageLoadPath = '/fundraisingpagenew/DrHoness?targetdevice=web';

        } catch (err) {
            JustGiving.WriteConsole(err);
        }
    }
</script>
